{
    "version": 2,
    "builds": [
        {
            "src": "/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "status": 301,
            "headers": {
                "Location": "/api/products/"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/server.ts"
        }
    ]
}